{% extends 'front-end/app.twig' %}

{% block nav %}
    {% include 'front-end/partials/navuser.twig' %}
{% endblock %}

{% block title %} Not Found - MIT Book Store {% endblock %}

{% block content %}
<div class="content-wrapper">	
    <section class="content">
      <div class="error-page">
        <h2 class="headline text-yellow">{{error}} 404</h2>

        <div class="error-content">
          <h3><i class="fa fa-warning text-yellow"></i> Oops! Page not found.</h3>

          <p>
            We could not find the page you were looking for.
            Meanwhile, you may <a href="{{path_for('home')}}">back to home</a>.
          </p>
        </div>
        <!-- /.error-content -->
      </div>
      <!-- /.error-page -->
    </section>
</div>
{% endblock %}